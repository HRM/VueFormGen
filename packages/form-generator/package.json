{
  "name": "vue-form-gen",
  "version": "1.0.1",
  "type": "module",
  "homepage": "https://hrm.github.io/VueFormGen/",
  "license": "MIT",
  "keywords": [
    "vue",
    "form",
    "generator",
    "json",
    "schema"
  ],
  "description": "Vue 3 form generator based on JSON schema",
  "repository": {
    "url": "https://github.com/HRM/VueFormGen"
  },
  "contributors": [
    {
      "name": "Tamás Török",
      "email": "tamas.torok.w@outlook.com"
    }
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test:unit": "vitest"
  },
  "dependencies": {
    "vue": "^3.5.13",
    "jsonschema": "^1.5.0"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.10.7",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.5.3",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.2"
  }
}
